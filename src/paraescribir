    async function obtenerLasProvincias(){
   
      
       const urlProvincias = 'https://www.el-tiempo.net/api/json/v2/provincias'
       
      const response = await fetch(urlProvincias);
      const post = await response.json();  
      var json = JSON.stringify(post.provincias);
      return json
   
   
   
     
     
       
   }